# API Configuration
VITE_API_URL=http://localhost:3001/api

# OAuth Configuration (Optional - for production)
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_GITHUB_CLIENT_ID=your_github_client_id
VITE_FACEBOOK_APP_ID=your_facebook_app_id

# App Configuration
VITE_APP_NAME=Neuro360
VITE_APP_URL=http://localhost:3000

# AWS (S3) Configuration
# WARNING: Do not commit real credentials. For production, issue presigned URLs from a backend.
VITE_AWS_REGION=ap-south-1
VITE_AWS_BUCKET_NAME=neuro360-reports-demo
VITE_AWS_ACCESS_KEY_ID=your_aws_access_key_id
VITE_AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# AWS DynamoDB Configuration
# Table names for DynamoDB (optional - will use localStorage if not configured)
VITE_DYNAMODB_TABLE_CLINICS=neuro360-clinics
VITE_DYNAMODB_TABLE_PATIENTS=neuro360-patients
VITE_DYNAMODB_TABLE_REPORTS=neuro360-reports
VITE_DYNAMODB_TABLE_PAYMENTS=neuro360-payments
VITE_DYNAMODB_TABLE_SUPER_ADMINS=neuro360-super-admins
VITE_DYNAMODB_TABLE_SUBSCRIPTIONS=neuro360-subscriptions
VITE_DYNAMODB_TABLE_USAGE=neuro360-usage

# Razorpay Configuration
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id
VITE_RAZORPAY_KEY_SECRET=your_razorpay_key_secret

# Email Service Configuration
VITE_EMAILJS_SECURE_TOKEN=your_emailjs_secure_token