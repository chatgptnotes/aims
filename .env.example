# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_ACCESS_TOKEN=your_supabase_access_token

# Vite/React App Config
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase CLI/Admin Config
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Database Direct Connection (for migrations)
DATABASE_URL=postgresql://postgres:[YOUR-DATABASE-PASSWORD]@db.[PROJECT-REF].supabase.co:5432/postgres

# App Environment
NODE_ENV=development
PORT=3000

# Payment Integration - Razorpay
# IMPORTANT: These are LIVE credentials - NEVER commit actual keys to git!
# Get your keys from: https://dashboard.razorpay.com/app/keys
VITE_RAZORPAY_KEY_ID=rzp_live_YOUR_KEY_ID_HERE
VITE_RAZORPAY_SECRET=YOUR_SECRET_KEY_HERE

# For development/testing, use test keys:
# VITE_RAZORPAY_KEY_ID=rzp_test_YOUR_KEY_ID_HERE
# VITE_RAZORPAY_SECRET=YOUR_TEST_SECRET_KEY_HERE

# Supabase Storage Configuration
VITE_SUPABASE_STORAGE_BUCKET=patient-reports

# SECURITY NOTES:
# 1. NEVER commit .env file to git (it's in .gitignore)
# 2. Use test keys (rzp_test_*) for development
# 3. Use live keys (rzp_live_*) only in production
# 4. Keep your secret key secure - it has full API access
# 5. Rotate keys immediately if compromised
