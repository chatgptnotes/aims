{
  "name": "@aims/web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "AIMS - Asset Information Management System for P&ID Analysis",
  "scripts": {
    "dev": "vite",
    "dev:direct": "node node_modules/vite/bin/vite.js",
    "dev:npx": "npx vite",
    "dev:backend": "cd server && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:backend\" --names \"frontend,backend\" --prefix-colors \"cyan,magenta\"",
    "start:backend": "cd server && npm start",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:limits": "node test-upload-limits.js",
    "migrate:trigger": "node apply-migration.js",
    "verify:razorpay": "node verify-razorpay-config.js",
    "verify:storage": "node verify-single-bucket.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.6.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.14",
    "js-cookie": "^3.0.5",
