    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "react-table": "^7.8.0",
    "recharts": "^2.12.7",
    "smtpjs": "^0.0.1",
    "sonner": "^2.0.7",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
